{
  "name": "tengo",
  "displayName": "tengo",
  "description": "Tengo syntax highlighting",
  "publisher": "lissein",
  "repository": {
    "type": "git",
    "url": "https://github.com/lissein/vscode-tengo"
  },
  "icon": "logo.png",
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tengo",
        "aliases": [
          "Tengo",
          "tengo"
        ],
        "extensions": [
          ".tengo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tengo",
        "scopeName": "source.tengo",
        "path": "./syntaxes/tengo.tmLanguage.json"
      }
    ]
  }
}
