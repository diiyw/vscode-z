{
    "name": "z",
    "displayName": "Z language",
    "description": "Z syntax highlighting and formatting",
    "publisher": "diiyw",
    "repository": {
        "type": "git",
        "url": "https://github.com/diiyw/vscode-z"
    },
    "icon": "logo.png",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.100.0"
    },
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "contributes": {
        "languages": [
            {
                "id": "z",
                "aliases": [
                    "Z",
                    "z"
                ],
                "extensions": [
                    ".z"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "z",
                "scopeName": "source.z",
                "path": "./syntax/z.tmLanguage.json"
            }
        ],
        "formatters": [
            {
                "language": "z",
                "extension": ".z"
            }
        ],
        "customEditorColors": {
            "[*]": {
                "textMateRules": [
                    {
                        "scope": "variable.other.z",
                        "settings": {
                            "foreground": "#569CD6"
                        }
                    }
                ]
            }
        }
    },
    "devDependencies": {
        "@types/vscode": "^1.100.0",
        "typescript": "^5.0.0"
    },
    "main": "./dist/extension.js",
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "license": "MIT",
    "dependencies": {
        "antlr4ts": "0.5.0-alpha.4"
    }
}