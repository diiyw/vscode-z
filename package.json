{
    "name": "z",
    "displayName": "Z language",
    "description": "Z syntax highlighting",
    "publisher": "diiyw",
    "repository": {
        "type": "git",
        "url": "https://github.com/diiyw/vscode-z"
    },
    "icon": "logo.png",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.100.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "z",
                "aliases": [
                    "Z",
                    "z"
                ],
                "extensions": [
                    ".z"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "z",
                "scopeName": "source.z",
                "path": "./syntax/z.tmLanguage.json"
            }
        ],
        "customEditorColors": {
            "[*]": {
                "textMateRules": [
                    {
                        "scope": "variable.other.z",
                        "settings": {
                            "foreground": "#569CD6"
                        }
                    }
                ]
            }
        }
    },
    "license": "MIT"
}